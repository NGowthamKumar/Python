class Node:
    def __init_(self,data):
        self.data=data
        self.left=None
        self.right=None
def print_mytree(root):
    if root!=None:
        print_mytree(root.left)
        print(root.data)
        print_mytree(root.right)
def insert(root,data):
    if root==None:
        return Node(data)
    if data<root.data:
        root.left=insert(root.left,data)
    else:
        root.right=insert(root.right,data)
    return root
def delete(root,data):
    if(data>root.data):
        root.right=delete(root.right)
    elif data<root.data:
        root.left=delete(root.left,data)
    else:
        if root.left==None:
            placeholder=root.right
            root=None
            return placeholder
         elif root.right==None:
             placeholder=root.left
             root=None
            return placeholder
    placeholder=findMin(root.right)
    root.data=placeholder.data
    root.right=delete(root.right,placeholder.data)
